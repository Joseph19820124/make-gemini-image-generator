# Google AI Studio API配置
GOOGLE_AI_API_KEY=your_api_key_here
GOOGLE_CLOUD_PROJECT_ID=your_project_id
GOOGLE_CLOUD_REGION=us-central1

# Make.com配置
MAKE_WEBHOOK_URL=https://hook.make.com/your_webhook
MAKE_API_KEY=your_make_api_key

# 服务器配置
PORT=3000
NODE_ENV=development

# 图片存储配置
IMAGE_STORAGE_BUCKET=your_storage_bucket
IMAGE_STORAGE_PATH=generated-images

# 高级设置
MAX_IMAGE_SIZE=10485760
DEFAULT_IMAGE_RESOLUTION=1024x1024
MAX_BATCH_SIZE=10
API_RATE_LIMIT=100

# 调试设置
DEBUG=gemini:*
LOG_LEVEL=info